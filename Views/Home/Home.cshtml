@model List<QuotesProj.Models.Quotes>
<html>
<head>
    <title>Profile</title>
    <link href="~/content/style.css" rel="stylesheet" type="text/css">
</head>

<body>
    <div class="main">
        <div class="page">
            <div class="header">
                <div class="header-img">
                    <h1>Best Quotes</h1>
                    <p>online repository of user contributed quotes</p>
                </div>
                <div class="menu">

                    <ul>
                        <li><a href="/Home/Home">Home</a></li>

                        <li><a href="/SearchByCatogary/SearchForMembers">Quotes Images</a></li>
                        <li><a href="/Contact_us/Contact_usForMembers">Contact Us</a></li>
                        <li><a href="/Profile/Profile">Profile</a></li>
                        <li><a href="/Post/Post">Post</a></li>
                        <li><a href="/Home/Logout">Logout</a></li>
                        <li style="color:white; margin-left:45%">Hi, @Request.Cookies["Username"].Value</li>
                    </ul>
                </div>
            </div>

            <div class="content">
                <div class="left-panel">
                    <div class="left-panel-in">


                        <h2>AllQuotes</h2>

                        @if (Model != null)
                        {
                            <table cellpadding="15" border="0">
                                @foreach (var x in Model)
                                {
                                    <tr>
                                        <td>@x.quote_text</td>
                                        <td><b>@x.author_name</b></td>

                                    </tr>
                                }
                            </table>

                        }



                        <table cellpadding="15" border="0"></table>



                        @*</font>*@


                    </div>
                </div>
                <div class="right-panel">
                    <div class="right-panel-in">

                        <h3>Categories</h3>
                        <ul>
                            <li><a href="/Category/Category?type=best">Best<br></a></li>
                            <li><a href="/Category/Category?type=sad">Sad<br></a></li>
                            <li><a href="/Category/Category?type=education">Education<br></a></li>
                            <li><a href="/Category/Category?type=love">Love<br></a></li>
                            <li><a href="/Category/Category?type=frindship">Friendship<br></a></li>
                            <li><a href="/SearchByCatogary/SearchByCatogary">Search<br></a></li>
                        </ul>

                        <p>&nbsp;</p>
                        <p>&nbsp;</p>
                        <p>&nbsp;</p>
                        <p>&nbsp;</p>
                        <p>&nbsp;</p>
                        <p>&nbsp;</p>
                    </div>
                </div>
            </div>


            <div class="footer">
                <!--DO NOT Remove The Footer Links-->
                <!--Designed by--><a href="http://www.htmltemplates.net">
                    <img src="~/images/footnote.gif" class="copyright" alt="html templates">
                </a>
                <p>
                    &copy; Copyright 2010. Designed by <a target="_blank"
                                                          href="http://www.htmltemplates.net">
                        htmltemplates.n

                        et
                    </a>
                    <!--In partnership with--><a href="http://websitetemplates.net">
                        <img src="~/images/footnote.gif" class="copyright"
                             alt="website templates">
                    </a>
                    <!--DO NOT Remove The Footer Links-->
                </p>
            </div>
        </div>
    </div>
</body>
</html>